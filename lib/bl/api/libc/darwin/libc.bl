S_ISDIR :: fn (mode: u32) bool #inline {
	return (mode & _S_IFMT) == _S_IFDIR;
}

S_ISREG :: fn (mode: u32) bool #inline {
	return (mode & _S_IFMT) == _S_IFREG;
}

